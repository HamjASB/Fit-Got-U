<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Fragment.MyPageFragment">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="160dp"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/mp_profile"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:maxWidth="120dp"
            android:maxHeight="120dp"
            android:minWidth="120dp"
            android:minHeight="120dp"
            android:src="@drawable/icon8"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/mp_name"
            android:layout_width="30dp"
            android:layout_height="19dp"
            android:layout_marginStart="7dp"
            android:layout_marginLeft="7dp"
            android:layout_marginTop="24dp"
            android:text="이름"
            android:textColor="#000000"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/mp_profile"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/mp_name2"
            android:layout_width="60dp"
            android:layout_height="19dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="12dp"
            android:layout_marginRight="12dp"
            android:text="TextView"
            app:layout_constraintEnd_toStartOf="@+id/mp_isuser2"
            app:layout_constraintStart_toEndOf="@+id/mp_profile"
            app:layout_constraintTop_toBottomOf="@+id/mp_name" />

        <TextView
            android:id="@+id/mp_isuser"
            android:layout_width="55dp"
            android:layout_height="19dp"
            android:layout_marginStart="35dp"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="24dp"
            android:text="회원유형"
            android:textColor="#000000"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/mp_name"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/mp_isuser2"
            android:layout_width="60dp"
            android:layout_height="19dp"
            android:layout_marginStart="4dp"
            android:layout_marginLeft="4dp"
            android:layout_marginTop="10dp"
            android:text="TextView"
            app:layout_constraintStart_toEndOf="@+id/mp_name2"
            app:layout_constraintTop_toBottomOf="@+id/mp_isuser" />

        <TextView
            android:id="@+id/mp_age"
            android:layout_width="42dp"
            android:layout_height="19dp"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:text="나이대"
            android:textColor="#000000"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/mp_isuser"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/mp_age2"
            android:layout_width="60dp"
            android:layout_height="19dp"
            android:layout_marginStart="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toEndOf="@+id/mp_isuser2"
            app:layout_constraintTop_toBottomOf="@+id/mp_age" />

        <TextView
            android:id="@+id/mp_address"
            android:layout_width="30dp"
            android:layout_height="19dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="10dp"
            android:text="주소"
            android:textColor="#000000"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/mp_address2"
            app:layout_constraintStart_toEndOf="@+id/mp_profile"
            app:layout_constraintTop_toBottomOf="@+id/mp_name2" />

        <TextView
            android:id="@+id/mp_address2"
            android:layout_width="110dp"
            android:layout_height="23dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="5dp"
            android:layout_marginRight="5dp"
            android:text="TextView"
            app:layout_constraintEnd_toStartOf="@+id/mp_spec2"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/mp_profile"
            app:layout_constraintTop_toBottomOf="@+id/mp_address" />

        <TextView
            android:id="@+id/mp_spec"
            android:layout_width="47dp"
            android:layout_height="19dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginBottom="10dp"
            android:text="키/체중"
            android:textColor="#000000"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/mp_spec2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toEndOf="@+id/mp_address"
            app:layout_constraintTop_toBottomOf="@+id/mp_age2" />

        <TextView
            android:id="@+id/mp_spec2"
            android:layout_width="131dp"
            android:layout_height="23dp"
            android:layout_marginStart="2dp"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="5dp"
            android:layout_marginRight="5dp"
            android:text="TextView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/mp_address2"
            app:layout_constraintTop_toBottomOf="@+id/mp_spec" />

    </android.support.constraint.ConstraintLayout>

    <ListView
        android:id="@+id/mp_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
        tools:layout_editor_absoluteX="0dp" />
</LinearLayout>